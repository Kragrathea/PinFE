              | View
            .dropdown-menu(aria-labelledby="navbarDropdown")
              a.dropdown-item(href="/") Select (Cab)
              a.dropdown-item(href="/?view=dt") Select (Desktop) 
              .dropdown-divider
              a.dropdown-item(href="/playfield") Playfield
              a.dropdown-item(href="/wheel") Wheel
              a.dropdown-item(href="/backglass") Backglass
              a.dropdown-item(href="http://localhost:9090/") DMD
