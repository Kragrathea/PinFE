              a.dropdown-item(href="/tables") Tables
              a.dropdown-item(href="/wheels") Wheels
              a.dropdown-item(href="/backglasses") Backglasses
              a.dropdown-item(href="/master") Master
              .dropdown-divider
              a.dropdown-item(href="#") Settings
              a.dropdown-item(href="http://localhost:3030") Storage
        form.form-inline.my-2.my-lg-0
