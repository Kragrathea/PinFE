          //- button.btn.btn-outline-success.my-2.my-sm-0(type="submit" ) Search
        button.btn(id='speech' onClick='talkWithApp()')
          i.fa.fa-microphone(aria-hidden="true")
          div.pulse-ring(style='display:none')
          
    block content

